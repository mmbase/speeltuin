<?xml version="1.0" encoding="UTF-8"?>
<statsconfig name="formas">
<!-- 
   This is the configuration file for the pagestats application.
   Logfiles can be generated by including stats/log.jsp in your jsp pages.
-->
<!--
   File extension of the log files. In case it is empty all files in the log directory will be parsed 
   Lines in the logfiles starting with a # sign are not parsed.
-->
<extension>txt</extension>
<!-- The directory where the log files can be found -->
<logdir>C:/data/didactor/webapps/formas/logs</logdir>
   <!-- 
      The delimiter used to seperate the tags. 
      Use a delimiter that is not inside the tags or the values. E.g. ';' or '|' 
   -->
	<delimiter>;</delimiter>
   <!--
      Specify the tags that follow the date=...;user=...;ip=...;session=... tags.
      Not every tag has to be present on every logline.
      Also the ip=... and session=... tags are not mandatory.
      The tags in the loglines do not have to follow the order below.
   -->
	<tags>
		<tag>nav</tag>
		<tag>subnav</tag>
		<tag>schooltype</tag>
		<tag>competence</tag>
		<tag>coretask</tag>
		<tag>testlevel</tag>
		<tag>testtype</tag>
		<tag>test</tag>
		<tag>page</tag>
	</tags>
   <!--
      Specify the tag that signals a session start. After this tag has been found, the sessionid (if present) is used
      to check if this is really a new session or if the users simply came back to the index page.
      The session end can have one of the following reasons: TIMEOUT, FILEEND or NEWDAY.
      When a FILEEND is found then all sessions and subtraces inside the session are finished by the actually last activity of that user before
      the fileend.
   -->
	<sessionstart tag="nav" value="Index" />
   <!--
      Specify the subtraces that have to be analyzed seperately.
      A subtrace is active as long as its values are found in the logfile, and no values from the other subtraces are found in between.
      1. Subtrace have to be part of one session i.e. they can not be spread over two or more sessions.
      2. Every subtrace can be finished by one of the following reasons
         - SUBTRACEPASS : if last <value> value for this subtrace occuried
         - NEWSUBTRACECAME : if value from another subtrace occuried (in the same session of course)
         - FILEEND or NEWDAY : if log file finished or new day detected
      3. When a subtrace last value occured twice or more in subsequent loglines then the subtrace is finished with SUBTRACEPASS
         reason by first occurence. All other last values ignored.
         (This to prevent from creating new subtrace sessions in the case of multiple last values)
   -->
   <subtraces>	
   	<subtrace name="Observatieopdracht">
   		<usetag>page</usetag>
   		<value>Observatieopdracht</value>
   		<value>Feedback op de observatieopdracht</value>
   		<value>Einde observatieopdracht</value>
   		<value>Overall oordeel observatieopdracht</value>
   		<value>Einde overall oordeel observatieopdracht</value>
   	</subtrace>
   	<subtrace name="Analyseopdracht">
   		<usetag>page</usetag>
   		<value>Analyseopdracht</value>
   		<value>Feedback op de analyseopdracht</value>
   		<value>Einde analyseopdracht</value>
   		<value>Overall oordeel analyseopdracht</value>
   		<value>Einde overall oordeel analyseopdracht</value>
   	</subtrace>
   	<subtrace name="Completeeropdracht">
   		<usetag>page</usetag>
   		<value>Completeeropdracht</value>
   		<value>Feedback op de completeeropdracht</value>
   		<value>Resultaten van de completeeropdracht</value>
   		<value>Overall oordeel complementeeropdracht</value>
   		<value>Einde complementeeropdracht</value>
   	</subtrace>
   	<subtrace name="Praktijkopdracht">
   		<usetag>page</usetag>
   		<value>Praktijkopdracht</value>
   		<value>Welke gedragingen zijn passend bij uw klasse situatie</value>
   		<value>Score formulier praktijkopdracht</value>
   		<value>Einde praktijkopdracht</value>
   	</subtrace>
   </subtraces>
</statsconfig>