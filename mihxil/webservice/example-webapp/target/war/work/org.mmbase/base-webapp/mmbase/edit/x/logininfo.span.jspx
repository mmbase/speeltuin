<span class="logininfo" xmlns:mm="http://www.mmbase.org/mmbase-taglib-2.0">
  <mm:import externid="logout" />
  <mm:present referid="logout">
    <mm:cloud method="logout" />
    <mm:redirect referids="$referids" />
  </mm:present>

  <mm:cloudinfo type="user" /> (<mm:cloudinfo type="rank" />)
  <mm:link referids="$referids">
    <mm:param name="logout" />
    <a href="${_}" class="logout"><span class="navigate">logout</span></a>
  </mm:link>
</span>