<?xml version="1.0"?>
<html
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:jsp="http://java.sun.com/JSP/Page"
    xmlns:c="http://java.sun.com/jsp/jstl/core"
    xmlns:mm="http://www.mmbase.org/mmbase-taglib-2.0">
  <mm:import externid="n" />
  <mm:import externid="component" required="true" />
  <mm:import externid="block"     required="true" />
  <mm:cloud>
    <mm:node referid="n">
      <head>
        <mm:component name="${component}" block="${block}" render="head" />
      </head>
      <body>
        <mm:component name="${component}" block="${block}" render="body" />
      </body>
    </mm:node>
  </mm:cloud>
</html>
