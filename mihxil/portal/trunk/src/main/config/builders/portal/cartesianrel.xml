  <names>
    <singular xml:lang="nl">CartesischRel</singular>
    <singular xml:lang="en">CartesianRel</singular>
    <plural xml:lang="nl">CartesischRels</plural>
    <plural xml:lang="en">CartesianRels</plural>
  </names>

  <descriptions>
    <description xml:lang="en">Basic relation containing three dimensional cartesian coordinates (x, y, z)</description>
    <description xml:lang="nl">Basisrelatietype met  (driedimensionale) Cartesische co√∂rdinaten (x, y, z)</description>
  </descriptions>

  <properties>
  </properties>

  <fieldlist>
    <field name="x">
      <descriptions>
        <description xml:lang="en">Value of x-position</description>
        <description xml:lang="nl">Waarde van de x-positie</description>
      </descriptions>
      <gui>
        <guiname xml:lang="nl">Xwaarde</guiname>
        <guiname xml:lang="en">Xvalue</guiname>
