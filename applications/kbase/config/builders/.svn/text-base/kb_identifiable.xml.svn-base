<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder maintainer="ernst.bunders" name="kb_identifiable" version="1" extends="kb_displayable">
  <status>active</status>
  <classfile>Dummy</classfile>
  <searchage>1000</searchage>
  <names>
    <singular xml:lang="en">kbase identifiable</singular>
    <plural xml:lang="en">kbase identifiable</plural>
  </names>
  <descriptions>
    <description xml:lang="nl">om een persoon met een object te associeren</description>
    <description xml:lang="en">to associate a person with an object</description>
  </descriptions>
  <properties>
    <property name="max-nodes-from-query">1000</property>
  </properties>
  <fieldlist>
        <field>
      <gui>
        <guiname xml:lang="en">name</guiname>
        <guitype>string</guitype>
      </gui>
      <editor>
        <positions>
          <input>5</input>
          <list>5</list>
          <search>5</search>
        </positions>
      </editor>
      <db>
        <name xml:lang="en">name</name>
        <type key="false" notnull="false" size="100" state="persistent">STRING</type>
      </db>
    </field>
    
    <field>
      <gui>
        <guiname xml:lang="en">email</guiname>
        <guitype>string</guitype>
      </gui>
      <editor>
        <positions>
          <input>6</input>
          <list>6</list>
          <search>6</search>
        </positions>
      </editor>
      <db>
        <name xml:lang="en">email</name>
        <type key="false" notnull="false" size="100" state="persistent">STRING</type>
      </db>
    </field>
    
    <field>
      <descriptions>
        <description xml:lang="en">This field is for personal notes regarding the object.</description>
      </descriptions>
      <gui>
        <guiname xml:lang="en">comment</guiname>
        <guitype>string</guitype>
      </gui>
      <editor>
        <positions>
          <input>7</input>
          <list>7</list>
          <search>7</search>
        </positions>
      </editor>
      <db>
        <name xml:lang="en">comment</name>
        <type key="false" notnull="false" size="3000" state="persistent">STRING</type>
      </db>
    </field>    
  </fieldlist>

  </builder>
