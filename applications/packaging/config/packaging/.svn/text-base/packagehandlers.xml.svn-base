<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE packagehandlers PUBLIC "-//MMBase/DTD packagehandlers config 1.0//EN" "http://www.mmbase.org/dtd/packagehandlers_1_0.dtd">
<packagehandlers>
	<packagehandler type="display/html" class="org.mmbase.applications.packaging.packagehandlers.DisplayHtmlPackage">
		<description>Installs/Uninstalls html pages</description>
	</packagehandler>
	<packagehandler type="cloud/model" class="org.mmbase.applications.packaging.packagehandlers.CloudModelPackage">
		<description>Installs/Uninstalls mmbase cloud models</description>
	</packagehandler>
	<packagehandler type="data/apps1" class="org.mmbase.applications.packaging.packagehandlers.DataApps1Package">
		<description>Installs/Uninstalls mmbase content data</description>
	</packagehandler>
	<packagehandler type="function/set" class="org.mmbase.applications.packaging.packagehandlers.FunctionSetPackage">
		<description>Installs/Uninstalls mmbase function sets</description>
	</packagehandler>
	<packagehandler type="java/jar" class="org.mmbase.applications.packaging.packagehandlers.JavaJarPackage">
		<description>Installs/Uninstalls jars</description>
	</packagehandler>
        <packagehandler type="config/basic" class="org.mmbase.applications.packaging.packagehandlers.ConfigBasicPackage">
                <description>Installs/Uninstalls basic config files</description>
        </packagehandler>
</packagehandlers>
