body
{
	font-family: "Lucida Grande", Verdana, Arial, sans-serif;
	font-size: 10px;
	background-color: white;
	display: float;
	margin: 0px;
	padding: 0px;
}

td
{
	font-family: "Lucida Grande", Verdana, Arial, sans-serif;
	font-size: 10px;
}

p
{
	font-size: 11px;
	text-align: left;
	margin: 0px;
	padding: 5px 0px 0px 0px;
}

ul
{
	margin-top: 10px;
	margin-bottom: 10px;
}

dt
{
	margin-left: 20px;
	font-weight: bold;
	color: #5D5D5D;
	font-size: 14px;
	text-decoration: underline;
}

dd
{
	margin-left: 20px;
	margin-bottom: 5px;
	font-size: 12px;
}

blockquote
{
	font-size: 12px;
	text-align: left;
	background: #EFEFEF;
}

ul
{
	font-size: 11px;
	text-align: left;
}

ol
{
	font-size: 11px;
	text-align: left;
}

h1
{
	font-size: 24px;
	font-weight: bold;
	color: #003333;
	text-align: left;
	padding-botom: 5px;
}

h2
{
	font-size: 18px;
	font-weight: bold;
	color: #003333;
	text-align: left;
}

h3
{
	font-size: 16px;
	font-weight: bold;
	color: #003333;
	text-align: left;
}

h4
{
	font-size: 14px;
	font-weight: bold;
	color: #003333;
	text-align: left;
}

h5
{
	font-size: 12px;
	font-weight: bold;
	color: #003333;
	text-align: left;
}

h6
{
	font-size: 10px;
	font-weight: bold;
	color: #003333;
	text-align: left;
}

h1, h2, h3, h4, h5, h6 { margin-bottom: 5px; }

b
{
	font-weight: bold;
	color: #003333;
}

strong
{
	font-weight: bold;
	color: #CCEB6F;
}

.layout { width: 100%; }

.layout, .layout th, 	.layout td
{
	border: 0px;
	padding: 0px;
	margin: 0px;
	background-color: white;
	text-align: left;
	vertical-align: top;
	font-size: 11px;
}

.list
{
	border: 1px solid #797868;
	padding: 0px;
}

.list tr th
{
	margin: 0px;
	padding: 3px;
	border: 1px solid #797868;
	font-size: 12px;
	background-color: #CCEB6F;
	color: #3B3B3B;
}

.list td
{
	background-color: #E7EBDE;
	font-size: 11px;
	color: black;
	border: 1px solid #797868;
	padding-left: 4px;
}

.list .listsearch td
{
	background-color: #E4E174;
	padding: 3px;
}

.list tr .listpaging
{
	background-color: #E4E174;
	padding: 3px;
}

.relationcontainer
{
	margin-top: 10px;
	width: 200px;
	border: 1px solid #797868;
}

.relationcontainer th
{
	background-color: #CCEB6F;
	border: 0px;
	padding: 3px;
	padding-left: 5px;
	padding-right: 5px;
	margin: 0px;
}

.relationcontainer td
{
	border: 0px;
	border-top: 1px solid #D8D8D8;
	padding: 3px;
	margin-left: 5px;
	padding-right: 5px;
	margin: 0px;
}

.subnav { height: 28px; }

.subnav td
{
	border: 1px solid white;
	background-color: #E7EBDE;
	text-align: center;
	padding-top: 5px;
}

.subnav td a
{
	font-size: 10px;
	color: black;
	font-family: "Lucida Grande", Arial, Helvetica, sans-serif;
}

.subnav  td a:link { color: black; }
.subnav td a:visited { color: black; }
.subnav td a:hover { color: #797868; }

.subnav tr .selected
{
	background-color: #CCEB6F;
	color: black;
	font-size: 11px;
	font-family: "Lucida Grande", Arial, Helvetica, sans-serif;
}

/*
	breadcrum and links
*/

.breadcrum
{
	font-family: "Lucida Grande", Arial, Helvetica, sans-serif;
	font-size: 10px;
	color: #000;
	padding: 2px 0px 2px 5px;
}

.breadcrumbar
{
	background-color: #CCEB6F;
	width: 100%;
	height: 18px;
}

.breadcrumbar td
{
	background-color: #CCEB6F;
	color: #000;
	height: 18px;
	border-width: 0px 0px 1px 0px;
	border-color: #000;
}

.breadcrum td a { color: #000; }

a:link
{
	color: #7A9B15;
	text-decoration: none;
	font-weight: bold;
}

a:visited
{
	color: #5B7410;
	text-decoration: none;
	font-weight: bold;
}

a:hover
{
	color: #797868;
	text-decoration: none;
}

/*
	/ breadcrum and links
*/

.content
{
	border-color: #333;
	border-width: 0px 1px 1px 0px;
	border-style: solid;
}

.intro
{
	font-size: 11px;
	font-weight: bold;
}

/*
	Form items etc
*/

input
{
	text-align: left;
	font-family: "Lucide Grande", Arial, sans-serif;
	font-size: 11px;
	background-color: #D9DDDE;
}

select
{
	font-family: "Lucide Grande", Arial, sans-serif;
	font-size: 11px;
	background-color: #D9DDDE;
}

textarea { background-color: #D9DDDE; }
