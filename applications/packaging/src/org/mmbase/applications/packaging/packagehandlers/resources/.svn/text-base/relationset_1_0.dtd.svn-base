<!--
     relationset.dtd
-->

<!ELEMENT relationset (relation*)>
<!ATTLIST relationset type CDATA #IMPLIED>
<!ATTLIST relationset exportsource CDATA #IMPLIED>
<!ATTLIST relationset timestamp CDATA #IMPLIED>

<!ELEMENT relation (field*)>
<!ATTLIST relation number CDATA #IMPLIED>
<!ATTLIST relation owner CDATA #IMPLIED>
<!ATTLIST relation snumber CDATA #IMPLIED>
<!ATTLIST relation dnumber CDATA #IMPLIED>
<!ATTLIST relation rtype CDATA #IMPLIED>
<!ATTLIST relation dir CDATA #IMPLIED>

<!ELEMENT field (#PCDATA)>
<!ATTLIST field name CDATA #IMPLIED>
