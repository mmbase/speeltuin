<!--
    packaging_cloud_model.dtd
-->

<!ELEMENT packaging_cloud_model (lastversion,lastdate,name,maintainer,description,license,releasenotes,installationnotes,modelfile,buildersdir,packagedepends,initiators,supporters,developers,contacts)>

<!ELEMENT lastversion (#PCDATA)>
<!ELEMENT lastdate (#PCDATA)>
<!ELEMENT modelfile (#PCDATA)>
<!ELEMENT buildersdir (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT maintainer (#PCDATA)>


<!ELEMENT description (#PCDATA)>

<!ELEMENT license (#PCDATA)>
<!ATTLIST license type CDATA #IMPLIED>
<!ATTLIST license name CDATA #IMPLIED>
<!ATTLIST license version CDATA #IMPLIED>

<!ELEMENT releasenotes (#PCDATA)>

<!ELEMENT installationnotes (#PCDATA)>

<!ELEMENT packagedepends (package*)>

<!ELEMENT package (#PCDATA)>
<!ATTLIST package name CDATA #IMPLIED>
<!ATTLIST package type CDATA #IMPLIED>
<!ATTLIST package maintainer CDATA #IMPLIED>
<!ATTLIST package version CDATA #IMPLIED>
<!ATTLIST package versionmode CDATA #IMPLIED>

<!ELEMENT initiators (initiator*)>
<!ELEMENT initiator (#PCDATA)>
<!ATTLIST initiator name CDATA #IMPLIED>
<!ATTLIST initiator company CDATA #IMPLIED>

<!ELEMENT supporters (supporter*)>
<!ELEMENT supporter (#PCDATA)>
<!ATTLIST supporter name CDATA #IMPLIED>
<!ATTLIST supporter company CDATA #IMPLIED>

<!ELEMENT developers (developer*)>
<!ELEMENT developer (#PCDATA)>
<!ATTLIST developer name CDATA #IMPLIED>
<!ATTLIST developer company CDATA #IMPLIED>
<!ATTLIST developer mailto CDATA #IMPLIED>

<!ELEMENT contacts (contact*)>
<!ELEMENT contact (#PCDATA)>
<!ATTLIST contact reason CDATA #IMPLIED>
<!ATTLIST contact name CDATA #IMPLIED>
<!ATTLIST contact mailto CDATA #IMPLIED>

