<!--
    packaging.dtd
-->

<!ELEMENT packaging (target*,package*,bundle*)>
<!ATTLIST packaging basedir CDATA #IMPLIED>

<!ELEMENT target (#PCDATA)>
<!ATTLIST target depends CDATA #IMPLIED>
<!ATTLIST target name CDATA #IMPLIED>

<!ELEMENT package (#PCDATA)>
<!ATTLIST package name CDATA #IMPLIED>
<!ATTLIST package type CDATA #IMPLIED>
<!ATTLIST package file CDATA #IMPLIED>

<!ELEMENT bundle (#PCDATA)>
<!ATTLIST bundle name CDATA #IMPLIED>
<!ATTLIST bundle type CDATA #IMPLIED>
<!ATTLIST bundle file CDATA #IMPLIED>
