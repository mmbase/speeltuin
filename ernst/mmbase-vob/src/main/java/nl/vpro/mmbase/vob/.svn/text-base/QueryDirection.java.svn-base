package nl.vpro.mmbase.vob;

/**
 * Specify the directionality of the query.
 * 
 * @author peter.maas@finalist.com
 */
public enum QueryDirection {
	SOURCE("source"), DESTINATION("destination"), BOTH("both");
	
	private String label;
	QueryDirection(String label){this.label = label;}
	/**
	 * returns a value that can be used in mmbase queries ('source', 'destination' or 'both)
	 */
	public String toString(){return label;}
}
