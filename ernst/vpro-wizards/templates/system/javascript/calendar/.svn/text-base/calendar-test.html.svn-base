<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title>Calendar test</title>
<link rel="stylesheet" type="text/css" href="calendar.css">
<style type="text/css">
.calendar_image {
	border: none;
	vertical-align: bottom;
	margin-bottom: 2px;
}
</style>
<script src="calendar.js"></script>
<script src="clock.js"></script>
<script>
window.onload = function() {
	calendar.create("dmy");
	calendar.attach("date");
	calendar.attach("geb");
	clock.create(15);
	clock.attach("time");
}

</script>
</head>
<body>
<form>
<input type="text" name="date" id="date" value="2-4-1965" size="10">
&nbsp;&nbsp;&nbsp;
<input type="text" name="geb" id="geb" value="20-11-1967" size="10">
<input type="text" name="time" id="time" value="9:30" size="6">
&nbsp;&nbsp;&nbsp;
<input type="text" name="finish" id="finish" value="2-4-1965"><a href="" onclick="calendar.toggle('finish'); return false" ondblclick="calendar.toggle('finish'); return false"><img  src="calendar.png" class="calendar_image"></a>
&nbsp;&nbsp;&nbsp;
<input type="text" name="bla" id="bla" value="0">
