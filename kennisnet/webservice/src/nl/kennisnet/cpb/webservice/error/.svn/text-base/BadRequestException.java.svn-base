package nl.kennisnet.cpb.webservice.error;

/**
 * Application Exception thrown when the input to the webservice was incorrect, 
 * for instance when the incoming atom-XML is malformed 
 * 
 * Log: not needed
 * 
 * @author dekok01
 * @version $Revision: 1.0
 */
public class BadRequestException extends WebserviceException {

   public BadRequestException(String message) {
      super(message);    
   }

   public BadRequestException(Throwable cause) {
      super(cause);
   }
}
