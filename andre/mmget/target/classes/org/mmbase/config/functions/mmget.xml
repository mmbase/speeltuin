<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE functionset PUBLIC "//MMBase - functionset //" "http://www.mmbase.org/dtd/functionset_1_0.dtd">
<functionset>
<version>0</version>
<description>Functionset to export MMBase sites to html files</description>
  
  <function name="download">
    <description>
      Exports a site and saves it in a directory.
    </description>
    <type>class</type>
    <class>org.mmbase.mmget.MMGet</class>
    <method>downloadSite</method>
    <param name="url"  type="String" description="Url to start with" />
    <param name="dir"  type="String" description="Directory to save resulting files to">/export</param>
    <return type="String" />
  </function>
  
  <function name="cancel">
    <description>
      Cancels the download.
    </description>
    <type>class</type>
    <class>org.mmbase.mmget.MMGet</class>
    <method>cancel</method>
  </function>

</functionset>
