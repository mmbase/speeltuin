<!-- 
   Example security.xml to switch on Cloud Context Security. No other configuration needed (Cloud Context Security
   is configured in the cloud)
   @version $Id: security.xml,v 1.1 2004-11-19 10:19:24 pierre Exp $   
  -->
<security active="true">
	<authentication class="org.mmbase.security.implementation.cloudcontext.Authenticate" url="" />	 
	<authorization  class="org.mmbase.security.implementation.cloudcontext.Verify"       url="" />
  <sharedsecret>pindakaas</sharedsecret><!-- unused? -->
</security>
