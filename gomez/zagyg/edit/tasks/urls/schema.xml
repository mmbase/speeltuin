<?xml version="1.0"?>
<!DOCTYPE form-schema PUBLIC "-//MMBase//DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
  <form-schema>
    <title xml:lang="en">Web Link</title>
    <title xml:lang="nl">Link</title>
    <field name="title" dtrequired="true" />
    <field name="url" dtrequired="true" />
    <field name="target" ftype="enum" >
      <optionlist select="targets" />
    </field>
    <field name="visible" ftype="enum" dtrequired="true">
      <optionlist select="show" />
    </field>
    <field name="begin" />
    <field name="end" />

    <list include ="tasks/images/list_icon.xml" />

  </form-schema>
