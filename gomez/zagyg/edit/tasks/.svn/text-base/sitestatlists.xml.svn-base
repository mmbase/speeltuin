<?xml version="1.0"?>
<!DOCTYPE lists PUBLIC "-//MMBase//DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<lists>
    <optionlist name="themes">
        <query xpath="/*@themes" />
    </optionlist>

    <optionlist name="labels">
        <query xpath="/*@labels" />
    </optionlist>

    <optionlist name="show">
      <option id="1">
        <prompt xml:lang="en">Show (using embargo fields)</prompt>
        <prompt xml:lang="nl">Tonen (gebruik embargo velden)</prompt>
      </option>
      <option id="0">
        <prompt xml:lang="en">Don't show</prompt>
        <prompt xml:lang="nl">Niet tonen</prompt>
      </option>
    </optionlist>
</lists>


