/* HCS editwizard stylesheet */

#divASContainer{position:absolute; width:110; height:150; overflow:hidden; top:335; left:140; clip:rect(0,110,150,0); visibility:hidden;}
#divASContent{position:absolute; top:0; left:0;}

body {
   color: #000099;
   background-color: white; /* lightblue */
}

p {
  color: #000099;
}

table {
  color:#000099;
}

h1 {
   color:white;
}
td {
  color:#000099;
}

.head		{ 
			font-family:"Trebuchet MS", Verdana, Arial, Helvetica, sans-serif; 
			font-weight:900; font-size:16px; letter-spacing:0em;
			}
.subhead	{ 
			font-family:"Trebuchet MS", Verdana, Arial, Helvetica, sans-serif;
			font-weight:900; font-size:14px; letter-spacing:0em; 
}

a {
 border:  none;
 font-weight: bold;
 text-decoration: none;
}

a:link {
 color: #000099;
}
a:active {
 color: #5050ff;
}
a:visited {
 color: #000099;
}
a:hover {
  color: red;
}

/* wizard */

td.fieldprompt {
   background-color: #fff;
   color: #000099;
}

.valid {
  color: #000099;
}

table.body tr.subtitle th {
  background-color: #EFEFEF;
}

/* for searchlist */

table.searchresult {
  background-color: #005;
}

table.searchresult tr.even td { background-color: #aaf; }
table.searchresult tr.odd  td { background-color: #bbf; }
table.searchresult tr.selected_even td { color: #fff; background-color: #33B; }
table.searchresult tr.selected_odd  td { color: #fff; background-color: #55A; }

/* for list */

td.listcanvas {
   background-color: white;
   border-color: #000000;
   border-width:2; 
}

tr.itemrow-hover td	{
   background-color: #EFEFEF;
   text-decoration:none;  
}

table.itemlist {
  border-style: none;
  border-spacing: 0;
}