<%@ taglib uri="http://www.mmbase.org/mmbase-taglib-1.0" prefix="mm" %><mm:import id="language">en</mm:import><mm:locale language="$language"><mm:cloud name="mmbase" method="http" rank="basic user" jspvar="cloud"><mm:import id="rank"><%=cloud.getUser().getRank()%></mm:import><html>   <head>        <title></title>        <link rel="stylesheet" type="text/css" href="css/editors.css">           </head>      <body bgcolor="#EFEFEF">   <%	   session.setAttribute("edit", cloud.getUser().getIdentifier());	   String userConstraint="mmbaseusers.username='"+cloud.getUser().getIdentifier()+"'";   %>   <table cellpadding=0 cellspacing=0>      <tr><td>Translations for <%= cloud.getUser().getIdentifier() %></td></tr>      <tr><td>&nbsp;</td></tr>      <tr><td><a href="<mm:url page="/edit/editwizards/index.jsp" />" target="_top">To editors</a></td></tr>      <tr><td>&nbsp;</td></tr>      <%--      <tr><td><a href="/index.jsp" target="_blank">Naar de website</a></td></tr>      <tr><td><a href="/dev/index.jsp" target="_blank">Naar de preview</a></td></tr>      <tr><td><a href="/editors/cache/flush.jsp" target="wizard">Flush de cache</a></td></tr>      --%>      <tr>         <td>            <a href="<mm:url page="/edit/index.jsp"/>" target="_top">Back to editor home</a>         </td>      </tr>      <tr>         <td>            <a href="<mm:url page="/edit/users/logout.jsp"/>" target="_top">Logout</a>         </td>      </tr>            <mm:import id="referrer">/edit/translations/empty.html</mm:import>      <mm:compare referid="rank" value="administrator">	      <mm:list path="editgroups">	         <tr><td>&nbsp;</td></tr>	         <tr>	            <td class="prikbord_text">	               <mm:field name="editgroups.name" />&nbsp;&nbsp;&nbsp;	            </td>	         </tr>	         <mm:field name="editgroups.number" jspvar="editgroups_number" vartype="String" write="false">	            <mm:list nodes="<%= editgroups_number %>" path="editgroups,posrel,edittranslate" orderby="posrel.pos" directions="UP">	               <mm:import id="editid" reset="true"><mm:field name="edittranslate.number"/></mm:import>	               	               <mm:import id="jsppage" reset="true"><mm:field name="edittranslate.jsppage"/></mm:import>	               <mm:import id="builder" reset="true"><mm:field name="edittranslate.builder"/></mm:import>	               <mm:import id="constraints" reset="true"><mm:field name="edittranslate.constraints"/></mm:import>	               <mm:import id="fields" reset="true"><mm:field name="edittranslate.fields"/></mm:import>	               <mm:import id="orderby" reset="true"><mm:field name="edittranslate.orderby"/></mm:import>	               <mm:import id="directions" reset="true"><mm:field name="edittranslate.directions"/></mm:import>	               <mm:import id="translationfields" reset="true"><mm:field name="edittranslate.translationfields"/></mm:import>	               <mm:import id="translationinput" reset="true"><mm:field name="edittranslate.translationinput"/></mm:import>	               <mm:import id="show" reset="true"><mm:field name="edittranslate.display"/></mm:import>	               	                  <tr>	                     <td>	                        <a href="<mm:url referids="editid" page="select.jsp"/>" target="search"	                          onClick='parent.wizard.location.href="<mm:url referids="builder,constraints,fields,orderby,directions,translationfields,translationinput,show,language,editid" page="$jsppage" />"'>	                           <mm:field name="edittranslate.title" />	                        </a>&nbsp;&nbsp;&nbsp;	                     </td>	                  </tr>	               </mm:field>	            </mm:list>	         </mm:field>	      </mm:list>      </mm:compare>      <!-- PUBLISHER -->      <mm:compare referid="rank" value="administrator" inverse="true">	    <mm:list path="mmbaseusers,posrel,editgroups" constraints="<%= userConstraint %>" orderby="posrel.pos" directions="UP">        <tr><td>&nbsp;</td></tr>         <tr>            <td class="prikbord_text">               <mm:field name="editgroups.name" />&nbsp;&nbsp;&nbsp;            </td>         </tr>         <mm:field name="editgroups.number" jspvar="editgroups_number" vartype="String" write="false">            <mm:list nodes="<%= editgroups_number %>" path="editgroups,posrel,edittranslate" orderby="posrel.pos" directions="UP">               <mm:import id="editid" reset="true"><mm:field name="edittranslate.number"/></mm:import>	               <mm:import id="jsppage" reset="true"><mm:field name="edittranslate.jsppage"/></mm:import>	               <mm:import id="builder" reset="true"><mm:field name="edittranslate.builder"/></mm:import>	               <mm:import id="constraints" reset="true"><mm:field name="edittranslate.constraints"/></mm:import>	               <mm:import id="fields" reset="true"><mm:field name="edittranslate.fields"/></mm:import>	               <mm:import id="orderby" reset="true"><mm:field name="edittranslate.orderby"/></mm:import>	               <mm:import id="directions" reset="true"><mm:field name="edittranslate.directions"/></mm:import>	               <mm:import id="translationfields" reset="true"><mm:field name="edittranslate.translationfields"/></mm:import>	               <mm:import id="translationinput" reset="true"><mm:field name="edittranslate.translationinput"/></mm:import>	               <mm:import id="show" reset="true"><mm:field name="edittranslate.display"/></mm:import>                  <tr>                     <td>                        <a href="<mm:url referids="edit" page="select.jsp"/>" target="search"                          onClick='parent.wizard.location.href="<mm:url referids="builder,constraints,fields,orderby,directions,translationfields,translationinput,show,language,editid" page="$jsppage" />"'>                        </a>&nbsp;&nbsp;&nbsp;                     </td>                  </tr>               </mm:field>            </mm:list>         </mm:field>         </mm:list>      </mm:compare>   </table></body></html></mm:cloud></mm:locale>